<%@ tag %><%@ include file="/WEB-INF/jsp/tagDirectives.jsp" %>

/* tags/anc/personMenuStyle.tag */

#menu {
	list-style-type: none;
	padding-left: 0em;
	margin-bottom: 1.0em;
}

#menu li {
	display: inline;
	padding-right: 2em;
	padding-left: 0em;
}

#menu .selected {
	font-weight: bold;
} 

#menu .disabled {
	opacity: 0.5;
} 
		